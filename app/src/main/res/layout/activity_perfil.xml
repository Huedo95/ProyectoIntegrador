<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary"
    tools:context=".Perfil.Perfil">

    <Button
        android:id="@+id/EditarACC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/button5"
        android:layout_marginStart="61dp"
        android:background="@color/colorPrimary"
        android:drawableTop="@android:drawable/ic_menu_edit"
        android:onClick="acc"
        android:text="@string/txtCambiarPerfil" />

    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="205dp"
        android:layout_marginEnd="59dp"
        android:background="@color/colorPrimary"
        android:drawableTop="@android:drawable/ic_menu_myplaces"
        android:onClick="ayuda"
        android:text="@string/txtEventos" />

    <ImageView
        android:id="@+id/fotoPerfil"
        android:layout_width="163dp"
        android:layout_height="228dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        app:srcCompat="@drawable/common_google_signin_btn_icon_dark_normal_background" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="16dp"
        android:orientation="vertical">

    </LinearLayout>


</RelativeLayout>